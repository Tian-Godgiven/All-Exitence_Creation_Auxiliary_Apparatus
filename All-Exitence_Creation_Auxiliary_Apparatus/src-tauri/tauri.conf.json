{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "all-exitence_creation_auxiliary_apparatus",
  "version": "0.1.0",
  "identifier": "com.all-exitence-creation-auxiliary-apparatus.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "all-exitence_creation_auxiliary_apparatus",
        "width": 350,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "fs": {
        "requireLiteralLeadingDot": true
      }
  },
  "mainBinaryName": ""
  
}
